@Library ('vishant-library@master') _
node {

    // tools { 
    //     maven 'maven'
    // }

properties(
    dildo.call()
)

    stage('check user'){
        checkUser()
    }

    stage('hello world') {
        serviceName.call()
    }

    stage('Current Image Tag'){
        currentImageTag.call()
    }

    stage('Fetching Source Code'){
        fetchCode.call()
    }

    stage('Let\'s Build') {
        build.call()
    }

    stage('Build Docker Image') {
        dockerCMD.call()
    }

    stage('Finally Let\'s deploy service to k8s Cluster'){
        deploy.call()
    }

    stage('Pod Status') {
        podStatus.call()
    }
}
